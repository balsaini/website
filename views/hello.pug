doctype html
html(lang='en')
  head
    title Hello World

  body
    h1 hello World
    h1=3*3
    h2 more awesome
    div
      a(href='http://google.com') Google

    -var names = ['joe', 'ball'];
    -var nums = [1,2,3];
    ul.myclass
      each n in names
        li=n

    ol
          each n, idx in nums
            li=n*idx
